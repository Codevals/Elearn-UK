{% extends 'base.html.twig' %}

{% block title %}Td{% endblock %}

{% block body %}
    <h1>Td</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ td.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ td.titre }}</td>
            </tr>
            <tr>
                <th>DocumentName</th>
                <td>{{ td.documentName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ td.updatedAt ? td.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ td.createdAt ? td.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('td_index') }}">back to list</a>

    <a href="{{ path('td_edit', {'id': td.id}) }}">edit</a>

    {{ include('td/_delete_form.html.twig') }}
